# Database
TCE_DATABASE_URL=postgresql+psycopg2://tce:tce@postgres:5432/tce

# EVM provider (Websocket)
# e.g. wss://eth-mainnet.g.alchemy.com/v2/KEY or wss://mainnet.infura.io/ws/v3/KEY
TCE_EVM_RPC_WS_URL=wss://eth-mainnet.g.alchemy.com/v2/your_key
TCE_EVM_CHAIN_ID=1

# Execution
TCE_DRY_RUN=true
TCE_EVM_PRIVATE_KEY=
TCE_EXECUTOR_ADDRESS=
TCE_SLIPPAGE_BPS=300
TCE_COPY_RATIO=0.2
TCE_MAX_NATIVE_IN_WEI=0
TCE_TX_DEADLINE_SECONDS=600
TCE_MAX_PRIORITY_FEE_GWEI=
TCE_MAX_FEE_GWEI=

# Aggregators
TCE_AGGREGATOR=
TCE_ONEINCH_BASE_URL=https://api.1inch.dev/swap/v5.2
TCE_ONEINCH_API_KEY=
TCE_ZEROEX_BASE_URL=https://api.0x.org
TCE_ZEROEX_BASE_URL_BASE_CHAIN=https://base.api.0x.org
TCE_ZEROEX_BASE_URL_POLYGON=https://polygon.api.0x.org
TCE_ZEROEX_BASE_URL_ARBITRUM=https://arbitrum.api.0x.org
TCE_ZEROEX_BASE_URL_OPTIMISM=https://optimism.api.0x.org

# Solana
TCE_SOL_RPC_URL=https://api.mainnet-beta.solana.com
TCE_SOL_EXECUTOR_PRIVATE_KEY=
TCE_SOL_EXECUTOR_PUBKEY=
TCE_JUPITER_QUOTE_URL=https://quote-api.jup.ag/v6/quote
TCE_JUPITER_SWAP_URL=https://quote-api.jup.ag/v6/swap
TCE_SOL_SUBSCRIBE_LOGS=false

# Discovery
TCE_DUNE_API_KEY=
TCE_DUNE_QUERY_ID=
TCE_BIRDEYE_API_KEY=
TCE_DISCOVER_INTERVAL_SEC=86400
TCE_DISCOVER_TOP_PERCENT=1.0
TCE_DISCOVER_MIN_TRADES=10
TCE_NANSEN_API_KEY=
TCE_NANSEN_BASE_URL=https://api.nansen.ai/v1
TCE_NANSEN_ENDPOINT_PATH=smart-money/top-traders?timeRange=7d
TCE_DISCOVER_ALLOWED_LABELS=
TCE_DISCOVER_DENIED_LABELS=
TCE_DISCOVER_CHAIN=
TCE_DISCOVER_ONCE=false
TCE_DISCOVER_PRUNE_OTHERS=false

# Alchemy (optional)
TCE_ALCHEMY_API_KEY=
TCE_ALCHEMY_BASE_URL=

# Per-chain aggregator overrides
TCE_AGGREGATOR_CHAIN_1=
TCE_AGGREGATOR_CHAIN_137=
TCE_AGGREGATOR_CHAIN_8453=

# Logging
TCE_LOG_LEVEL=INFO

# Optional Polygon WS RPC for concurrent watcher
TCE_POLYGON_EVM_RPC_WS_URL=

# Migrations (only read by entrypoint.sh; not part of AppSettings)
# Set to true ONLY on a single service (e.g., api) to avoid race conditions
# TCE_RUN_MIGRATIONS=true
